                    <span class="material-symbols-rounded">mic</span>
                    {{localization.nav_singers}}
                </a>
                <a class="nav-drawer-link" href="/rankings/producers">
                    <span class="material-symbols-rounded">music_note</span>
                    {{localization.nav_producers}}
                </a>
